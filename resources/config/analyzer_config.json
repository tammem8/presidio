{
    "nlp_engine_name": "spacy",
    "model_name": "en_core_web_sm",
    "language": "en",
    "context_enhancer_count": 10,
    "recognizers_to_keep": [
        "SpacyRecognizer",
        "DateRecognizer",
        "PhoneRecognizer",
        "EmailRecognizer",
        "CreditCardRecognizer",
        "IbanRecognizer"
    ],
    "custom_recognizers": [
        {
            "entity_type": "SSL",
            "patterns": [
                {
                    "name": "ssl_key_pattern",
                    "regex": "-----BEGIN\\s+(RSA\\s+)?PRIVATE\\s+KEY-----[\\s\\S]*?-----END\\s+(RSA\\s+)?PRIVATE\\s+KEY-----",
                    "score": 0.6
                }
            ]
        },
        {
            "entity_type": "BIOMEDICAL",
            "patterns": [
                {
                    "name": "fingerprint_pattern",
                    "regex": "SHA256:[0-9A-Fa-f]{2}(?::[0-9A-Fa-f]{2}){15}",
                    "score": 0.6
                }
            ]
        }
    ],
    "entity_mappings": [
        {"source": "SSL_KEY", "target": "SSL"},
        {"source": "FINGERPRINT_HASH", "target": "BIOMEDICAL"}
    ]
}
